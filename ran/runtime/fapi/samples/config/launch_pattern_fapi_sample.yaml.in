# Launch pattern YAML template for fapi_sample integration test
# Python script substitutes cell configs and slot config at runtime
---
data_buf_opt: 1

# Cell configuration test vectors (array of test vector filenames)
Cell_Configs: @CELL_CONFIGS@

# Initialization phase (empty - no init patterns needed)
INIT: {}

# Schedule definition
# CONFIG.request is sent during initialization (before slot 0)
# Slots 0-19: Send DL/UL requests from test vector
# Pattern repeats every 20 slots
SCHED:
- slot: 0
  # Cell configurations for slot 0
  config: @SLOT_CONFIG@
- {slot: 1, config: {}}
- {slot: 2, config: {}}
- {slot: 3, config: {}}
- {slot: 4, config: {}}
- {slot: 5, config: {}}
- {slot: 6, config: {}}
- {slot: 7, config: {}}
- {slot: 8, config: {}}
- {slot: 9, config: {}}
- {slot: 10, config: {}}
- {slot: 11, config: {}}
- {slot: 12, config: {}}
- {slot: 13, config: {}}
- {slot: 14, config: {}}
- {slot: 15, config: {}}
- {slot: 16, config: {}}
- {slot: 17, config: {}}
- {slot: 18, config: {}}
- {slot: 19, config: {}}

